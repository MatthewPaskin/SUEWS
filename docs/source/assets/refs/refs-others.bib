@inbook{CN88,
 author = {Campbell, Gaylon S. and Norman, John M.},
 editor = {Campbell, Gaylon S. and Norman, John M.},
 address = {New York, NY},
 title = {Wind},
 isbn = {978-1-4612-1626-1},
 url = {https://doi.org/10.1007/978-1-4612-1626-1_5},
 abstract = {As living organisms, we are most acutely aware of three things about the wind. We know that it exerts a force on us and other objects against which it blows, it is effective in transporting heat from us, and it is highly variable in space and time. A fourth property of the wind, less obvious to the casual observer, but essential to terrestrial life as we know it, is its effective mixing of the atmospheric boundary layer of the earth. This can be illustrated by a simple example. On a hot summer day about 10 kilograms (550 moles) of water can be evaporated into the atmosphere from each square meter of vegetated ground surface. This amount of water would increase the vapor concentration in a 100 m thick air layer by 100 g m$-$3 (136 mmol/mol) if there were no transport out of this layer or condensation within it. This is much more water than the air could hold at normal temperature. The observed increase in vapor concentration in the first 100 meters of the atmosphere is typically less than 1 g m$-$3, so we can see how effective the atmosphere is for transporting and mixing. A similar calculation (Monteith, 1973) shows that photosynthesis in a normally growing crop would use all of the CO2 in a 30 m air layer above a crop in a day, yet measured CO2 concentrations have diurnal fluctuations of 15 percent or less. Without the vertical turbulent transport of heat, water vapor, CO2, oxygen, and other atmospheric constituents, the microenvironment we live in would be very inhospitable.},
 language = {en},
 booktitle = {An Introduction to Environmental Biophysics},
 publisher = {Springer New York},
 year = {1998},
 doi = {10.1007/978-1-4612-1626-1_5},
 keywords = {Planetary Boundary Layer, Roughness Length, Vapor Concentration, Wind Profile, Wind Speed},
 pages = {63-75},
 source = {Crossref},
 chapter = {Wind},
}

@article{B71,
 author = {Businger, J. A. and Wyngaard, J. C. and Izumi, Y. and Bradley, E. F.},
 title = {Flux-Profile Relationships in the Atmospheric Surface Layer},
 volume = {28},
 issn = {0022-4928, 1520-0469},
 url = {https://doi.org/10.1175/1520-0469(1971)028<0181:fprita>2.0.co;2},
 doi = {10.1175/1520-0469(1971)028<0181:fprita>2.0.co;2},
 abstract = {{\textless }section class="abstract"{\textgreater }{\textless }h2 class="abstractTitle text-title my-1" id="d2675526e64"{\textgreater }Abstract{\textless }/h2{\textgreater }{\textless }p{\textgreater }Wind and temperature profiles for a wide range of stability conditions have been analyzed in the context of Monin-Obukhov similarity theory. Direct measurements of heat and momentum fluxes enabled determination of the Obukhov length {\textless }em{\textgreater }L{\textless }/em{\textgreater }, a key independent variable in the steady-state, horizontally homogeneous, atmospheric surface layer. The free constants in several interpolation formulas can be adjusted to give excellent fits to the wind and temperature gradient data. The behavior of the gradients under neutral conditions is unusual, however, and indicates that von K\'arm\'an's constant is $\sim$0.35, rather than 0.40 as usually assumed, and that the ratio of eddy diffusivities for heat and momentum at neutrality is $\sim$1.35, compared to the often-suggested value of 1.0. The gradient Richardson number, computed from the profiles, and the Obukhov stability parameter {\textless }em{\textgreater }z{\textless }/em{\textgreater }/{\textless }em{\textgreater }L{\textless }/em{\textgreater }, computed from the measured fluxes, are found to be related approximately linearly under unstable conditions. For stable conditions the Richard on number approaches a limit of $\sim$0.21 as stability increases. A comparison between profile-derived and measured fluxes shows good agreement over the entire stability range of the observations.{\textless }/p{\textgreater }{\textless }/section{\textgreater }},
 language = {EN},
 number = {2},
 urldate = {2021-10-28},
 journal = {J. Atmos. Sci.},
 month = mar,
 year = {1971},
 pages = {181-189},
 file = {Businger et al\_1971\_Flux-Profile Relationships in the Atmospheric Surface Layer.pdf:/Users/sunt05/Dropbox/zotero-lib/Businger et al\_1971\_Flux-Profile Relationships in the Atmospheric Surface Layer.pdf:application/pdf},
 source = {Crossref},
 publisher = {American Meteorological Society},
}

